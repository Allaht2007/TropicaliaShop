<%- include("../partials/header.ejs"); %>
<%- include("../partials/navbar.ejs"); %>
<head>
    <link rel="stylesheet" href="./style/info.css">
</head>
<body>
    <section class="principal">
        <span class="ingrid-darling-regular nameLogo spanTamCenterInfo">Meus dados</span>
        <div class="containerOrigin">
            <form action="" method="" class="formInfos">
                <div class="containerLeft">
                    <img src="../src/Component2.webp"  class="imgUser" alt="Usuario">
                    <label id="lbInfo">Meus dados</label>
                    <input type="text" placeholder="Nome de Usuário">
                    <input type="text" placeholder="Email">
                    <button type="submit">Salvar alterações</button>
                </div>
            </form>
            <div class="partLeft">
                <div class="left2">
                    <span>Meu Endereço</span>
                    <label for="cep">CEP</label>
                    <input type="number" id="cep" name="cep" required>
                    <label for="uf">Estado</label>
                    <input type="text"  id="uf" name="uf" required>
                    <label for="city">Cidade</label>
                    <input type="text" id="city"  name="city" required>
                    <label for="rua">Rua</label>
                    <input type="text" id="rua"  name="rua" required>
                    <label for="num">Número</label>
                    <input type="text " id="num"  name="num" required>
                </div>
            </div>  
        </div>
    </section>
</body>